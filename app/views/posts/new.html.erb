<div class="col lg-12"><h1>Create a new post, dickface</h1></div>
<div class="col lg-12">
  <%= form_for([@category, @post]) do |f| %>
    <%= f.label :title%>
    <%= f.text_field :title%><br>
    <%= f.label :email%>
    <%= f.text_field :email%><br>
    <%= f.label :price%>
    <%= f.text_field :price%><br>
    <%= f.label :content%>
    <%= f.text_field :content%><br>
    <%= f.label :url%>
    <%= f.text_field :url%><br>
    <%= f.submit%><br>
  <% end %>
    
</div>
